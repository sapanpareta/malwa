---
import "../styles/global.css";
import Header from "../components/Header.jsx";
import Services from "../components/Services.astro";
import Footer from "../components/Footer.astro";
import PhotoGallery from "../components/PhotoGallery.astro";
---

<html lang="en" class="dark">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Services - Malwa Packers and Movers Indore</title>
    
  </head>

  <body class=" bg-background text-content">
    <Header client:load />

    <main class="pt-16">
      <Services />
      <PhotoGallery />
         <section>
        <div class="sp-container mb-10 text-center">
          <h2
            class="bg-gradient-to-tr from-pink-400 to-purple-500 bg-clip-text text-transparent"
          >
            Find Us On Google Map
          </h2>
        </div>
        <div class="sp-container">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d58861.45437538074!2d75.883906!3d22.771285!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x396302b8fc14d4ab%3A0x78241a7f973a4ddf!2sMalwa%20Roadways%20Packers%20and%20Movers!5e0!3m2!1sen!2sin!4v1757054638606!5m2!1sen!2sin"
            width="100%"
            height="450"
            style="border:0;"
            allowfullscreen=""
            loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
      </section>
    </main>
     <Footer />
  </body>
</html>
